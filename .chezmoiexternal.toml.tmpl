{{ if ne .chezmoi.os "windows" }}
[".config/zsh/plugins/zsh-syntax-highlighting"]
	type = "git-repo"
	url = "https://github.com/zsh-users/zsh-syntax-highlighting.git"
	exact = true
	striComponents = 1
	refreshPeriod = "168h"
[".config/zsh/plugins/zsh-autosuggestions"]
	type = "git-repo"
	url = "https://github.com/zsh-users/zsh-autosuggestions.git"
	exact = true
	striComponents = 1
	refreshPeriod = "168h"
[".config/nvim"]
{{ else }}
["AppData/Local/nvim"]
{{ end }}
	type = "git-repo"
	url = "https://github.com/kicre/nvim.git"
	exact = true
	striComponents = 1
[".config/alacritty/theme"]
	type = "git-repo"
	url = "https://github.com/alacritty/alacritty-theme.git"
	exact = true
	striComponents = 1
	refreshPeriod = "168h"
